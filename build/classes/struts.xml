<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <include file="struts-default.xml"/>
    <package name="Mysql" extends="struts-default">
        <action name="updateInfo" class="Mysql.Teacher_Mysql" method="updateInfo">
            <result type="chain">viewInfo</result>
        </action>
        <action name="viewSchedule" class="Mysql.Teacher_Mysql" method="viewSchedule">
            <result>../Teacher/updateSche.jsp</result>
        </action>
        <action name="modPwd" class="Mysql.Teacher_Mysql" method="modPwd">
            <result>adminPwd.jsp</result>
        </action>
        <action name="viewAppointment" class="Mysql.Teacher_Mysql" method="viewAppointment">
            <result>../Teacher/viewApp.jsp</result>
        </action>
        <action name="manageAppointment" class="Mysql.Teacher_Mysql" method="manageAppointment">
            <result>../Teacher/manageApp.jsp</result>
        </action>
        <action name="removeAppointment" class="Mysql.Teacher_Mysql" method="removeAppointment">
            <result type="chain">manageAppointment</result>
        </action>
        <action name="viewInfo" class="Mysql.Teacher_Mysql" method="viewInfo">
            <result>../Teacher/updateInfo.jsp</result>
        </action>
		<action name="setBusy" class="Mysql.Teacher_Mysql" method="setBusy">
            <result type="chain">viewSchedule</result>
        </action>
        <action name="setFree" class="Mysql.Teacher_Mysql" method="setFree">
            <result type="chain">viewSchedule</result>
        </action>
    </package>
</struts>