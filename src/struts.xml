<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <include file="struts-default.xml"/>
    <package name="Mysql" extends="struts-default">
        <action name="updateInfo" class="Mysql.Teacher_Mysql" method="updateInfo">
            <result type="chain">viewInfo</result>
        </action>
        <action name="viewSchedule" class="Mysql.Teacher_Mysql" method="viewSchedule">
            <result>../Teacher/updateSche.jsp</result>
        </action>
        <action name="viewInfo" class="Mysql.Teacher_Mysql" method="viewInfo">
            <result>../Teacher/updateInfo.jsp</result>
        </action>
		<action name="setBusy" class="Mysql.Teacher_Mysql" method="setBusy">
            <result type="chain">viewSchedule</result>
        </action>
        <action name="setFree" class="Mysql.Teacher_Mysql" method="setFree">
            <result type="chain">viewSchedule</result>
        <action name="getTimeTable" class="action.GetTimeTableAction">
            <result>/Student/timetable.jsp</result>
        </action>
    </package>
</struts>